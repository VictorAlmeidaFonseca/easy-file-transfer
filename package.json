{
  "name": "file-transfer-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron electron/main.cjs\"",
    "dev:debug": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron --inspect=9222 electron/main.cjs\"",
    "build": "vite build",
    "build:force": "vite --force",
    "preview": "vite preview",
    "start": "electron electron/main.cjs",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/postcss": "^4.0.17",
    "@tsconfig/svelte": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "electron": "^35.1.2",
    "flowbite": "^3.1.2",
    "flowbite-svelte": "^0.48.4",
    "flowbite-svelte-icons": "^2.1.0",
    "postcss": "^8.5.3",
    "svelte": "^5.20.2",
    "svelte-check": "^4.1.4",
    "tailwindcss": "^4.0.17",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vite-plugin-static-copy": "^2.3.0",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.0.17",
    "express": "^4.21.2",
    "local-ip-address": "^1.0.1",
    "multer": "1.4.5-lts.2",
    "qrcode": "^1.5.4",
    "svelte-preprocess": "^6.0.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "electron",
      "esbuild",
      "svelte-preprocess"
    ]
  }
}